{
    "files.eol": "\n",
    "files.autoGuessEncoding": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.fixAll.eslint": "explicit"
    },
    "eslint.validate": [
        "typescript",
        "typescriptreact",
        "javascript",
        "javascriptreact"
    ],
    "eslint.useFlatConfig": true
}
